<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="300.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="btnGenerate" layoutX="7.0" layoutY="139.0" mnemonicParsing="false" onAction="#createDocFile" text="Сделать круто" />
      <Button fx:id="btnLoadFileTxt" layoutX="7.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnLoadTxtFileClick" prefHeight="49.0" prefWidth="117.0" text="Загрузить файл с перменными" wrapText="true" />
      <Label fx:id="lblPathTxt" layoutX="134.0" layoutY="26.0" prefHeight="25.0" prefWidth="476.0" />
      <Button fx:id="btnSaveDocFile" layoutX="7.0" layoutY="76.0" mnemonicParsing="false" onAction="#btnSaveDocFileClick" prefHeight="49.0" prefWidth="117.0" text="Сохранить отчет в файл" wrapText="true" />
      <Label fx:id="lblPathDoc" layoutX="134.0" layoutY="88.0" prefHeight="25.0" prefWidth="476.0" />
   </children>
</Pane>
